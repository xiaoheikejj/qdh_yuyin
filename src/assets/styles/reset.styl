@import './main.styl'

html, body, button, div, span, input, textarea, h1, h2, h3, h4, h5, h6, p, img, b, i, ol, ul, li, form, label, table, tbody, tr, th, td, canvas, footer, header, nav, section {
    margin: 0;
    padding: 0;
    border: 0;
    outline: none;
    font-size: 12px;
    outline: none;
}

body{
  width: 100%;
  font-family: STYuanti-SC-Regular, "微软雅黑";
}

a {
    font-size: 12px;
    color: #fff;
    text-decoration: none;
}

li {
    list-style: none;
}

.clear::after {
    content: '';
    display: block;
    clear: both;
}

.amap-logo, .amap-copyright {
  display: none !important;
}


